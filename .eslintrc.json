{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "ignorePatterns": [
        "test/*"
    ],
    "rules": {
        "linebreak-style": "off",
        "semi": "off",
        "no-console": "off",
        "brace-style": "off",
        "radix": "off",
        "no-underscore-dangle": "off"
    }
}
